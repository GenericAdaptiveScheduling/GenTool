		<formula>
			<forall var = "v1" in = "pat_B">
				<not>
					<exists var = "v2" in = "pat_H">
						<bfunc name = "same">
							<param pos = "1" var = "v1" field = "subject" />
							<param pos = "2" var = "v2" field = "subject" />
						</bfunc>
					</exists>
				</not>
			</forall>
		</formula>
		<patterns>
			<pattern>
				<first type = "add" name = "pat_H" />
				<second type = "del" name = "pat_H" />
			</pattern>
			<pattern>
				<first type = "add" name = "pat_H" />
				<second type = "del" name = "pat_B" />
			</pattern>
			<pattern>
				<first type = "add" name = "pat_B" />
				<second type = "del" name = "pat_H" />
			</pattern>
			<pattern>
				<first type = "add" name = "pat_B" />
				<second type = "del" name = "pat_B" />
			</pattern>
		</patterns>
	</rule>
	
	<rule>
		<id>rule_equal_GI1</id>
		<formula>
