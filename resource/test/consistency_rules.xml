				<second type = "del" name = "pat_G" />
			</pattern>
			<pattern>
				<first type = "add" name = "pat_G" />
				<second type = "add" name = "pat_I" />
			</pattern>
			<pattern>
				<first type = "del" name = "pat_I" />
				<second type = "del" name = "pat_G" />
			</pattern>
			<pattern>
				<first type = "del" name = "pat_I" />
				<second type = "add" name = "pat_I" />
			</pattern>
		</patterns>
	</rule>
	<rule>
		<id>rule_equal_GI2</id>
		<formula>
			<forall var = "v1" in = "pat_I">
				<exists var = "v2" in = "pat_G">
					<bfunc name = "same">
						<param pos = "1" var = "v1" field = "subject" />
						<param pos = "2" var = "v2" field = "subject" />
					</bfunc>
				</exists>
			</forall>
		</formula>
		<patterns>
			<pattern>
				<first type = "add" name = "pat_I" />
